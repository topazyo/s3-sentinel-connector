{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */%POLLING_INTERVAL_MINUTES% * * * *"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 3,
    "minimumInterval": "00:00:30",
    "maximumInterval": "00:05:00"
  }
}
